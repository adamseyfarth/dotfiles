PATH="$HOME/.cargo/bin:$PATH"
PATH="$HOME/junest/bin:$PATH"
PATH="/usr/lib64/openmpi/bin:$PATH"
PATH="/usr/local/krb5/bin:/usr/local/ossh/bin:$PATH"
PATH="$HOME/anaconda3/bin:$PATH"
PATH="$HOME/.rakudobrew/bin:$PATH"
PATH="$HOME/aurelia/bin:$PATH"
export PATH="$HOME/bin:$HOME/.local/bin:$HOME/install/bin:$PATH:/opt/anaconda/bin"
